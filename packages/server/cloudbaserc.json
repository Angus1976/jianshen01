{
  "envId": "cloud1-4g2aaqb40446a63b",
  "functions": [
    {
      "name": "api",
      "runtime": "Nodejs10.15",
      "handler": "app.main_handler",
      "timeout": 30,
      "memorySize": 512,
      "installDependency": true,
      "ignore": [
        "node_modules",
        "node_modules/**/*",
        ".git"
      ],
      "triggers": [
        {
          "type": "http",
          "config": {
            "path": "/",
            "method": "any"
          }
        }
      ]
    }
  ]
}
